{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wps/contact-info",
	"title": "Contact info",
	"description": "Display phone and email",
	"category": "wps-blocks",
	"icon": "info-outline",
	"supports": {
		"anchor": true,
		"html": false,
		"spacing": {
			"margin": true,
			"padding": true
		},
		"color": {
			"background": true,
			"text": true,
			"link": true
		},
		"typography": {
			"fontSize": true,
			"fontFamily": true,
			"lineHeight": true,
			"fontStyle": true,
			"fontWeight": true,
			"textDecoration": true,
			"textTransform": true,
			"letterSpacing": true
		},
		"align": [
			"full"
		]
	},
	"keywords": ["Contact", "Info", "Phone", "Email"],
	"attributes": {
		"textAlign": {
			"type": "string"
		},
		"anchor": {
			"type": "string"
		},
		"showPhoneOne": {
			"type": "boolean",
			"default": true
		},
		"showPhoneTwo": {
			"type": "boolean"
		},
		"showPhonePlatform": {
			"type": "boolean"
		},
		"showEmailOne": {
			"type": "boolean",
			"default": true
		},
		"showEmailTwo": {
			"type": "boolean"
		},
		"enableUrl": {
			"type": "boolean"
		},
		"enableIcon": {
			"type": "boolean"
		}
	},
	"styles": [
		{ "name": "default", "label": "Vertical", "isDefault": true },
		{ "name": "horizontal", "label": "Horizontal" }
	],
	"style": "file:../../../build/block-library/contact-info/style.css",
	"editorScript": "file:../../../build/block-library/contact-info/index.js"
}
