{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wps/navigation",
	"title": "Main Navigation",
	"description": "Use the Navigation block to add one main menu to your site header",
	"category": "wps-blocks",
	"icon": "menu",
	"supports": {
		"anchor": true,
		"html": false,
		"align": ["wide", "full"],
		"color": {
			"background": true,
			"text": false,
			"link": true,
			"gradients": false
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"blockGap": true
		},
		"typography": {
			"fontSize": true
		}
	},
	"keywords": ["menu" ,"navigation"],
	"attributes": {
		"anchor": {
			"type": "string"
		},
		"menuId": {
			"type": "string"
		},
		"toggleButtonLocation": {
			"type": "string"
		},
		"displayBreakpoint": {
			"type": "number",
			"default": 1024
		}
	},
	"styles": [
		{ "name": "default", "label": "Default", "isDefault": true },
		{ "name": "underline", "label": "Underline" }
	],
	"style": "file:../../../build/block-library/navigation/style.css",
	"viewScript": "file:../../../build/block-library/navigation/frontend.js",
	"editorStyle": "file:../../../build/block-library/navigation/editor.css",
	"editorScript": "file:../../../build/block-library/navigation/index.js"
}
